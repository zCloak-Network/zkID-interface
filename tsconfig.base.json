{
  "extends": "@zcloak/dev/config/tsconfig.json",
  "compilerOptions": {
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "composite": false,
    "jsx": "react",
    "paths": {
      "@zkid/app": ["app/src"],
      "@zkid/app/*": ["app/src/*"],
      "@zkid/app-config": ["app-config/src"],
      "@zkid/app-config/*": ["app-config/src/*"],
      "@zkid/extension-core": ["extension-core/src"],
      "@zkid/extension-core/*": ["extension-core/src/*"],
      "@zkid/react-components": ["react-components/src"],
      "@zkid/react-components/*": ["react-components/src/*"],
      "@zkid/react-hooks": ["react-hooks/src"],
      "@zkid/react-hooks/*": ["react-hooks/src/*"],
      "@zkid/page-dashboard": ["page-dashboard/src"],
      "@zkid/page-dashboard/*": ["page-dashboard/src/*"],
      "@zkid/page-home": ["page-home/src"],
      "@zkid/page-home/*": ["page-home/src/*"],
      "@zkid/page-tutorial": ["page-tutorial/src"],
      "@zkid/page-tutorial/*": ["page-tutorial/src/*"]
    },
    "skipLibCheck": true
  }
}
